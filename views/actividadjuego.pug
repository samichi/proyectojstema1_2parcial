html
    head
        title="ChuChu TV Game"
        link(href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', rel='stylesheet')
        link(href='../public/css/chuchu.css', rel='stylesheet')
    body
        div.container
            div.row
                div(class="col-lg-10.col-xs-10.col-md-10")
                    img(src="../public/images/fondoUnido.png", alt="placeholder 960", class="img-responsive", width="100%")
                div.col-lg-2.col-xs-2.col-md-2
                     a(class="btn btn-warning glyphicon glyphicon-play" href="" role="button")
                     br
                     br
                     a(class="btn btn-success glyphicon glyphicon-picture" href="" role="button")
            div.row
            div.col-lg-3.col-xs-3.col-md-3
            div.col-lg-2.col-xs-2.col-md-2
                a(class="btn btn-primary" href="" role="button")
                    img(src="../public/", alt="", class="img-thumbnail", width="100px")
